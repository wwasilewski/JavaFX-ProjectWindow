<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="200.0" prefHeight="600.0"
           prefWidth="796.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="main.MainWindowController">
    <children>
        <Button fx:id="window1" mnemonicParsing="false" text="Window1" StackPane.alignment="CENTER"
                onAction="#openWindow1">
            <StackPane.margin>
                <Insets bottom="10.0" left="10.0" right="90.0" top="10.0"/>
            </StackPane.margin>
        </Button>
        <Button fx:id="window2" mnemonicParsing="false" text="Window2" StackPane.alignment="CENTER">
            <StackPane.margin>
                <Insets bottom="10.0" left="90.0" right="10.0" top="10.0"/>
            </StackPane.margin>
        </Button>
    </children>
</StackPane>

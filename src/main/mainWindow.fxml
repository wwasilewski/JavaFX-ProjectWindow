<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="0.0" prefHeight="600.0"
           prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="main.MainWindowController">
    <children>
        <Button fx:id="button1" mnemonicParsing="false" onAction="#buttonClicked" text="Button"
                StackPane.alignment="TOP_RIGHT">
            <StackPane.margin>
                <Insets right="10.0" top="60.0"/>
            </StackPane.margin>
        </Button>
        <Label fx:id="labelText" text="Label" StackPane.alignment="TOP_CENTER">
            <StackPane.margin>
                <Insets left="10.0" top="30.0"/>
            </StackPane.margin>
        </Label>
        <Button fx:id="button2" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#handleButtonAction"
                text="Show Boar" StackPane.alignment="BOTTOM_CENTER">
            <font>
                <Font size="14.0"/>
            </font>
            <StackPane.margin>
                <Insets bottom="10.0"/>
            </StackPane.margin>
        </Button>
        <ImageView fx:id="imageview" fitHeight="206.0" fitWidth="207.0" pickOnBounds="true" preserveRatio="true"
                   StackPane.alignment="CENTER"/>
        <TextField fx:id="textField1" maxWidth="400.0" minWidth="200.0" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="120.0"/>
            </StackPane.margin>
        </TextField>
        <RadioButton fx:id="female" mnemonicParsing="false" onAction="#radioClicked" text="Female"
                     StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="10.0" top="160.0"/>
            </StackPane.margin>
        </RadioButton>
        <RadioButton fx:id="male" mnemonicParsing="false" onAction="#radioClicked" text="Male"
                     StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="100.0" top="160.0"/>
            </StackPane.margin>
        </RadioButton>
        <CheckBox fx:id="chk1" mnemonicParsing="false" onAction="#checkboxClicked" text="ClickMe"
                  StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets bottom="10.0" left="10.0" right="100.0" top="190.0"/>
            </StackPane.margin>
        </CheckBox>
        <ImageView fx:id="imageview2" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"
                   StackPane.alignment="CENTER_RIGHT"/>
        <Button mnemonicParsing="false" opacity="0.5" prefWidth="50.0" style="-fx-background-radius: 0;"
                text="          " textOverrun="CLIP" StackPane.alignment="CENTER_LEFT">
            <StackPane.margin>
                <Insets left="10.0"/>
            </StackPane.margin>
        </Button>
        <Button mnemonicParsing="false" prefWidth="70.0" style="-fx-background-radius: 0;" text="              "
                StackPane.alignment="CENTER_LEFT">
            <StackPane.margin>
                <Insets left="60.0"/>
            </StackPane.margin>
        </Button>
        <Button mnemonicParsing="false" prefWidth="70.0" style="-fx-background-radius: 0;" text="            "
                StackPane.alignment="CENTER_LEFT">
            <StackPane.margin>
                <Insets left="130.0"/>
            </StackPane.margin>
        </Button>
    </children>
</StackPane>
